<html>
<head>
  <script src="/bundle.js"></script>
  <link rel="stylesheet" type="text/css" href="/style.css?v=1">
  <script>
    document.styleSheets[0].insertRule('{{=it.navSelector}} { background-color:#3E517A; }', 1);
    {{? it.setToken}}
    BundleEntry.Auth.authenticateUser('{{=it.setToken}}');
    {{?}}
  </script>
</head>
<body>
    <div id="header">
      <div id="header-inner">
        <span id="accountroot">
          Enable JavaScript to register an account and log in.
        </span>
        <a href="/">
          <span id="logo">
            <img src='/images/logocherry.png'></img>Blue Fruit
          </span>
        </a>
        <div class="clearboth"></div>
        <nav>
          <a href="/"><span class="navItem navHome">Homepage</span></a>
          <a href="/list"><span class="navItem navList">Full List</span></a>
          <a href="/submit"><span class="navItem navSubmit">Submit Games</span></a>
        </nav>
      </div>
    </div>
  <div id="wrapper">
    <div id="content">
      {{=it.content}}
    </div>
  </div>
</body>
</html>
