<p>
_("Order by:") 
{{? it.linkactive == "alpha"}}
_("Alphabetical") | <a href="/list/new">_("Date")</a>
{{?? it.linkactive == "new"}}
<a href="/list/alpha">_("Alphabetical")</a> | _("Date")
{{?}}
</p>
<table class="games publicgames">
  <tr>
    <th>_("Game")</th>
    <th>_("Creator(s)")</th>
    <th>_("Release Date")</th>
  </tr>
  {{~it.games :value:index}}
  <tr><td><a href="{{=value.link}}">{{=value.name}}</a></td><td>{{=value.authors}}</td><td>{{=value.approvedAt}}</td></tr>
  {{~}}
</table>
