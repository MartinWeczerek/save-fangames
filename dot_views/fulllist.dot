<p>{{=it.toplinks}}</p>
<table class="games publicgames">
  <tr>
    <th>Name</th>
    <th>Creator(s)</th>
    <th>Released</th>
  </tr>
  {{~it.games :value:index}}
  <tr><td><a href="{{=value.link}}">{{=value.name}}</a></td><td>{{=value.authors}}</td><td>{{=value.approvedAt}}</td></tr>
  {{~}}
</table>
